alias glibmm : : : :
	<cflags>"`pkg-config --cflags glibmm-2.4`"
	<linkflags>"`pkg-config --libs glibmm-2.4`"
	;

lib odbc : : <name>odbc ;

lib odbcpp :
	[ glob *.cpp ] :
	<cflags>-fPIC
	<library>glibmm 
	<library>odbc 
	<library>../libdbpp
	: :
	<include>.
	<library>glibmm 
	<library>../libdbpp
	;
